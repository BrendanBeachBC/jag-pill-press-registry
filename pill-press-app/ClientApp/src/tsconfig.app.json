{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "module": "es6",
    "baseUrl": "./",
    "target": "es5",
    "types": [
      "knockout"
    ],
    "paths": {
      "@app*": ["app*"],
      "@models*": ["app/models*"],
      "@services*": ["app/services*"],
      "@shared*": ["app/shared*"]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
